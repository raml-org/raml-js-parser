    "object-assign": "^4.1.0",
    "pluralize": "~7.0.0",
    "q": "1.5.1",
    "uritemplate": "~0.3.4"
  },
  "devDependencies": {
    "browserify": "~16.1.1",
    "chai": "~4.1.2",
    "chai-as-promised": "~7.1.1",
    "coffeeify": "~3.0.1",
    "grunt": "~1.0.2",
    "grunt-cli": "~1.2.0",
    "grunt-coffeelint": "~0.0.7",
    "grunt-contrib-coffee": "~2.0.0",
    "grunt-contrib-connect": "~1.0.2",
    "grunt-contrib-uglify": "~3.3.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-mocha-cli": "~4.0.0",
    "grunt-mocha-phantomjs": "~4.0.0",
    "grunt-shell": "~2.1.0",
    "load-grunt-tasks": "~3.5.2",
    "mocha": "~5.0.4",
    "mversion": "~1.10.1",
    "pre-commit": "~1.2.2"
  },
  "scripts": {
    "build": "grunt build",
    "mversion": "mversion",
